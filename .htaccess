<files ~ "^.*\.([Hh][Tt][Aa])">
order allow,deny
deny from all
satisfy all
</files>

Options -Indexes

<FilesMatch "^\.">
Deny from all

</FilesMatch>

<FilesMatch "bd.sql">
Deny from all

</FilesMatch>

<FilesMatch "bd.sql">
Deny from all

</FilesMatch>


<FilesMatch "composer.json">
Deny from all

</FilesMatch>

<Limit PUT DELETE PATCH >
Deny from all
</Limit>

<Limit OPTIONS CONNECT >
Deny from all
</Limit>

